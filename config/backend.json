      "GetConfigBackModels": {
        "properties": {
          "status_code": { "type": "integer", "title": "Status Code" },
          "msg": { "type": "string", "title": "Msg" },
          "data": {
            "items": { "$ref": "#/components/schemas/QueryConfigModels" },
            "type": "array",
            "title": "Data"
