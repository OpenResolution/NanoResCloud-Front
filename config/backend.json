          "access_token": { "type": "string", "title": "Access Token" },
          "refresh_token": { "type": "string", "title": "Refresh Token" },
          "expires_at": { "type": "string", "format": "date-time", "title": "Expires At" },
          "token_type": { "type": "string", "title": "Token Type" }
