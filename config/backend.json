        },
        "type": "object",
        "required": ["status_code", "msg", "data"],
        "title": "GetConfigBackModels"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": { "$ref": "#/components/schemas/ValidationError" },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "QueryConfigModels": {
        "properties": {
          "ConfigForm": { "$ref": "#/components/schemas/ConfigFormModels" },
          "config_id": { "type": "string", "title": "Config Id" },
          "create_at": { "type": "string", "format": "date-time", "title": "Create At" },
          "update_at": { "type": "string", "format": "date-time", "title": "Update At" }
        },
        "type": "object",
        "required": ["ConfigForm", "config_id", "create_at", "update_at"],
        "title": "QueryConfigModels"
      },
      "QueryPageModel": {
        "properties": {
